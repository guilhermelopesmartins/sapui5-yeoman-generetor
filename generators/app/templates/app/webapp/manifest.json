{
  "_version": "1.0.1",
  "sap.app": {
    "_version": "1.0.0",
    "id": "<%= appName %>",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "<%= appName %>",
    "description": "<%= shortName %>",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      {
        "Id": "sap_belize",
        "Description": "Belize"
      },
      {
        "Id": "sap_belize_plus",
        "Description": "Belize Plus"
      },
      {
        "Id": "sap_bluecrystal",
        "Description": "Blue Crystal"
      },
      {
        "Id": "sap_belize_hcb",
        "Description": "Preto"
      },
      {
        "Id": "sap_belize_hcw",
        "Description": "Branco"
      }
    ]
  },
  "sap.ui5": {
    "_version": "1.2.0",
    "rootView": "<%= appName %>.src.app.App",
    "config": {
      "serviceUrl": "webapp/model/data",
      "phpApi": "http://localhost/fluent.api/v1.php/"
    },
    "dependencies": {
      "minUI5Version": "1.34.0",
      "libs": {
        "sap.ui.core": {
          "minVersion": "1.34.0"
        },
        "sap.m": {
          "minVersion": "1.34.0"
        },
        "sap.ui.layout": {
          "minVersion": "1.34.0"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "<%= appName %>.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "pattern": "app",
          "name": "app",
          "target": "app"
        },
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "dashBoard",
          "name": "dashBoard",
          "target": "dashBoard"
        },
        {
          "pattern": "settings/:userName:",
          "name": "settings",
          "target": "settings"
        },
        {
          "pattern": "exemplos",
          "name": "exemplos",
          "target": "exemplos"
        },
        {
          "pattern": "languages",
          "name": "languages",
          "target": "languages"
        }
      ],
      "targets": {
        "dashBoard": {
          "viewName": "DashBoard",
          "viewLevel": 2,
          "transition": "flip",
          "viewPath": "<%= appName %>.src.pages.dashboard"
        },
        "settings": {
          "viewName": "Edit",
          "viewLevel": 3,
          "viewPath": "<%= appName %>.src.pages.settings",
          "transition": "flip"
        },
        "login": {
          "viewName": "Login",
          "viewLevel": 3,
          "viewPath": "<%= appName %>.src.pages.security",
          "transition": "flip"
        },
        "exemplos": {
          "viewName": "CustomControls",
          "viewLevel": 3,
          "viewPath": "<%= appName %>.src.pages.exemplos",
          "transition": "flip"
        },
        "app": {
          "viewName": "App",
          "viewLevel": 3,
          "viewPath": "<%= appName %>.src.app"
        },
        "languages": {
          "viewName": "Languages",
          "viewLevel": 3,
          "viewPath": "<%= appName %>.src.pages.languages"
        }
      }
    }
  },
  "name": "<%= appName %>",
  "short_name": "ROne",
  "start_url": "index.html",
  "display": "standalone",
  "orientation": "portrait",
  "icons": [
    {
      "src": "img/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "background_color": "#576574",
  "theme_color": "#4e8ef7"
}
